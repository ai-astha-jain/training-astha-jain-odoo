<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--sequence given to products-->
        <record id="seq_product_reference" model="ir.sequence">
            <field name="name">Product Reference</field>
            <field name="code">product.reference</field>
            <field name="prefix">BOOK-%(year)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!--sequence given to member-->
        <record id="seq_library_member" model="ir.sequence">
            <field name="name">library Membership</field>
            <field name="code">library.membership</field>
            <field name="prefix">BOOK-%(year)s-%(month)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
